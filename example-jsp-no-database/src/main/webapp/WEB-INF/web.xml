<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>MovieServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.MovieServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>MovieServlet</servlet-name>
    <url-pattern>/movies</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ResultServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.InputReadingExampleServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ResultServlet</servlet-name>
    <url-pattern>/inputreader</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CreateMovieServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.CreateMovieServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CreateMovieServlet</servlet-name>
    <url-pattern>/createmovie</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteMovieServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.DeleteMovieServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DeleteMovieServlet</servlet-name>
    <url-pattern>/deletemovie</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SelectMovieServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.SelectMovieServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SelectMovieServlet</servlet-name>
    <url-pattern>/selectmovie</url-pattern>
  </servlet-mapping>
</web-app>
